<h1>Listing Books</h1>
<%= link_to 'New book',new_book_path %>
<br>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Description</th>
    <th>Year</th>
  </tr>
 
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.description %></td>
      <td><%= book.year %></td>
    </tr>
  <% end %>
</table>